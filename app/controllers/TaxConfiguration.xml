<?xml version="1.0" encoding="utf-8"?>
<dataController name="TaxConfiguration" conflictDetection="overwriteChanges" label="Tax Configuration" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"TaxConfiguration"."TaxConfigurationID" "TaxConfigurationID"
	,"TaxConfiguration"."StaffAssignmentID" "StaffAssignmentID"
	,"StaffAssignment"."StaffClassificationID" "StaffAssignmentStaffClassificationID"
	,"StaffAssignmentDepartment"."DepartmentName" "DepartmentName"
	,"StaffAssignmentOrganizationPersonRole"."OrganisationID" "StaffAssignmentOrganizationPersonRoleOrganisationID"
	,"StaffAssignmentPositionType"."Description" "PositionType"
	,"StaffAssignmentStaffClassification"."Description" "Classification"
	,"TaxConfiguration"."TaxBasisID" "TaxBasisID"
	,"TaxBasis"."Description" "TaxBasis"
	,"TaxBasis"."Code" "TaxBasisCode"
	,"TaxConfiguration"."TaxCodeID" "TaxCodeID"
	,"TaxCode"."TaxCode" "TaxCode"
	,"TaxConfiguration"."NICategoryID" "NicategoryID"
	,"NICategory"."Description" "NICategory"
	,"NICategory"."Code" "NICode"
	,"TaxConfiguration"."TaxReference" "TaxReference"
	,"TaxConfiguration"."EffectiveDate" "EffectiveDate"
	,"TaxConfiguration"."ExpirationDate" "ExpirationDate"
	,"TaxConfiguration"."CreatedDate" "CreatedDate"
	,"TaxConfiguration"."CreatedBy" "CreatedBy"
	,"TaxConfiguration"."ModifiedDate" "ModifiedDate"
	,"TaxConfiguration"."ModifiedBy" "ModifiedBy"
from "dbo"."TaxConfiguration" "TaxConfiguration"
	left join "dbo"."K12StaffAssignment" "StaffAssignment" on "TaxConfiguration"."StaffAssignmentID" = "StaffAssignment"."StaffAssignmentID"
	left join "dbo"."Department" "StaffAssignmentDepartment" on "StaffAssignment"."DepartmentID" = "StaffAssignmentDepartment"."DepartmentID"
	left join "dbo"."OrganizationPersonRole" "StaffAssignmentOrganizationPersonRole" on "StaffAssignment"."OrganizationPersonRoleID" = "StaffAssignmentOrganizationPersonRole"."OrganizationPersonRoleID"
	left join "dbo"."zlkpClassroomPositionType" "StaffAssignmentPositionType" on "StaffAssignment"."PositionTypeID" = "StaffAssignmentPositionType"."RefClassroomPositionTypeId"
	left join "dbo"."zlkpStaffClassification" "StaffAssignmentStaffClassification" on "StaffAssignment"."StaffClassificationID" = "StaffAssignmentStaffClassification"."RefEducationStaffClassificationId"
	left join "dbo"."zlkpTaxBasis" "TaxBasis" on "TaxConfiguration"."TaxBasisID" = "TaxBasis"."TaxBasisID"
	left join "dbo"."zlkpNICategory" "NICategory" on "TaxConfiguration"."NICategoryID" = "NICategory"."NICategoryID"
	left join "dbo"."zlkpTaxCode" "TaxCode" on "TaxConfiguration"."TaxCodeID" = "TaxCode"."TaxCodeID"
]]></text>
    </command>
    <command id="TaxConfigurationIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="TaxConfigurationID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="TaxConfigurationID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Tax Configuration ID" readOnly="true" />
    <field name="StaffAssignmentID" type="Int32" allowNulls="false" label="Staff Assignment ID" showInSummary="true">
      <items style="Lookup" dataController="K12StaffAssignment" newDataView="createForm1" dataValueField="StaffAssignmentID" dataTextField="StaffAssignmentID" copy="StaffAssignmentStaffClassificationID=StaffClassificationID, DepartmentName=Department, StaffAssignmentOrganizationPersonRoleOrganisationID=OrganizationPersonRoleOrganisationID, PositionType=PositionType, Classification=Classification" />
    </field>
    <field name="StaffAssignmentStaffClassificationID" type="Int32" readOnly="true" label="Staff Assignment Staff Classification ID" />
    <field name="DepartmentName" type="String" readOnly="true" label="Department Name" length="25" />
    <field name="StaffAssignmentOrganizationPersonRoleOrganisationID" type="Int32" readOnly="true" label="Staff Assignment Organization Person Role Organisation ID" />
    <field name="PositionType" type="String" readOnly="true" label="Position Type" length="100" />
    <field name="Classification" type="String" readOnly="true" label="Classification" length="100" />
    <field name="TaxBasisID" type="Int32" label="Tax Basis ID" showInSummary="true">
      <items style="Lookup" dataController="TaxBasis" newDataView="createForm1" dataValueField="TaxBasisID" dataTextField="Description" copy="TaxBasisCode=Code" />
    </field>
    <field name="TaxBasis" type="String" readOnly="true" label="Tax Basis" length="50" />
    <field name="TaxBasisCode" type="String" readOnly="true" label="Tax Basis Code" length="10" />
    <field name="TaxCodeID" type="Int32" allowNulls="false" label="Tax Code ID" showInSummary="true">
      <items style="Lookup" dataController="TaxCode" newDataView="createForm1" dataValueField="TaxCodeID" dataTextField="TaxCode" />
    </field>
    <field name="TaxCode" type="String" readOnly="true" label="Tax Code" length="15" />
    <field name="NicategoryID" type="Int32" allowNulls="false" label="NI Category ID" showInSummary="true">
      <items style="Lookup" dataController="NICategory" newDataView="createForm1" dataValueField="NicategoryID" dataTextField="Description" copy="NICode=Code" />
    </field>
    <field name="NICategory" type="String" readOnly="true" label="NI Category" length="100" />
    <field name="NICode" type="String" readOnly="true" label="NI Code" length="5" />
    <field name="TaxReference" type="String" label="Tax Reference" length="20" showInSummary="true" />
    <field name="EffectiveDate" type="DateTime" allowNulls="false" label="Effective Date" />
    <field name="ExpirationDate" type="DateTime" label="Expiration Date" />
    <field name="CreatedDate" type="DateTime" allowNulls="false" dataFormatString="g" default="(getutcdate())" label="Created Date" />
    <field name="CreatedBy" type="String" allowNulls="false" default="(suser_sname())" label="Created By" length="100" />
    <field name="ModifiedDate" type="DateTime" dataFormatString="g" label="Modified Date" />
    <field name="ModifiedBy" type="String" label="Modified By" length="100" />
  </fields>
