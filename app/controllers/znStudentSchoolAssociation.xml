-- SET @Result_ExecuteOnClient = '$appfactory.find("StudentSchoolAssociation", "Controller").sync();'
SET @Result_ShowMessage = 'The students promotion process ' + CONVERT(NVARCHAR(12), GETDATE()) + ' ' + 'has completed successfully' 

