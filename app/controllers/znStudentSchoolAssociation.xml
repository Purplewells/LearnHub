--SET @Result_ExecuteOnClient = '$appfactory.find("StudentSchoolAssociation", "Controller").sync();'
SET @Result_ShowMessage = 'The students promotion process ' + CONVERT(NVARCHAR(12), GETDATE()) + ' ' + 'has completed successfully' 

]]></data>
      </action>
      <action id="a100" />
      <action commandName="SQL" headerText="RC-01 - Refresh Contacts" description="InTerm Enrollment" cssClass="material-icon-sync" confirmation="This action would refresh student contacts with the appropriate Grade Level information. Are you sure you want to continue ?" id="a105">
        <data><![CDATA[/*
	Version		Date :			Description	sync year group																				Author
	-- 0.1		18/09/2019		This process updates the current selected record and										Benjamin Ohene-Adu
								updates the gradelevel, CurrentYearStatus and the ExtendedRole								Benjamin Ohene-Adu
	-- 0.2		19/09/2019      This stage of the process updates the status of students with a current enrollment record
								CurrentYearGradeLevel, CurrentYearStatus and ExtendedRole
*/


EXEC [zusp_harmonise_grade_level_groups]
	SET @Result_ShowMessage = 'Update process for CYGs ' + ' ' + 'has completed successfully' ;
	SET @Result_Refresh = 1]]></data>
      </action>
      <action commandName="SQL" headerText="UnEnrol Selected Students" description="InTerm Enrollment" cssClass="material-icon-code_off" confirmation="This action would un-enroll selected students from school's registered list. Are you sure you want to continue ?" id="a103" whenKeySelected="true">
        <data><![CDATA[UPDATE opr
SET ExitDate = ssa.ExitWithdrawDate
FROM
OrganizationPersonRole OPR
INNER JOIN StudentSchoolAssociation ssa on opr.OrganizationPersonRoleID = opr.OrganizationPersonRoleID
INNER JOIN Person P ON P.PersonID = OPR.PersonID
INNER JOIN zlkpRole zr on zr.RoleId = opr.RoleID
WHERE SSA.ExitWithdrawDate <= GETDATE()
AND zr.RoleName in ('Student')
AND ssa.StudentEnrollmentID = @StudentEnrollmentID


-- UPDATE PERSON ENDDATES
UPDATE P
SET ExtendedLiveStatusEndDate = opr.ExitDate
    , CurrentYearStatus = 'No'
from Person p
left join OrganizationPersonRole opr on opr.PersonID = p.PersonID
left join zlkpRole zr on zr.RoleId = opr.RoleID
where zr.RoleName in ('Student')
and opr.ExitDate <= GETDATE()


/*
	Version		Date :			Description																					Author
	-- 0.1		18/09/2019		This process updates the current selected record and										Benjamin Ohene-Adu
								updates the gradelevel, CurrentYearStatus and the ExtendedRole								Benjamin Ohene-Adu
	-- 0.2		19/09/2019      This stage of the process updates the status of students with a current enrollment record
								CurrentYearGradeLevel, CurrentYearStatus and ExtendedRole
*/


EXEC [zusp_harmonise_grade_level_groups]
	SET @Result_ShowMessage = 'Update process for CYGs ' + ' ' + 'has completed successfully' ;
        SET @Result_ShowMessage = 'The students expiration process ' + CONVERT(NVARCHAR(12), GETDATE()) + ' ' + 'has completed successfully' 	
        SET @Result_Refresh = 1


]]></data>
      </action>
