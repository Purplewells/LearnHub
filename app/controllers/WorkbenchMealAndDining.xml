<?xml version="1.0" encoding="utf-8"?>
<dataController name="WorkbenchMealAndDining" conflictDetection="overwriteChanges" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
