            <dataField fieldName="TargetValue" columns="15" />
            <dataField fieldName="MeasurementTypeID" aliasFieldName="MeasurementType" />
            <dataField fieldName="CourseID" aliasFieldName="CourseTitle" />
            <dataField fieldName="CalendarSessionID" aliasFieldName="SessionCode" />
