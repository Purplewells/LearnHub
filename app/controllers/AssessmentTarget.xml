<?xml version="1.0" encoding="utf-8"?>
<dataController name="AssessmentTarget" conflictDetection="overwriteChanges" label="Assessment Target" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"AssessmentTarget"."AssessmentTargetID" "AssessmentTargetID"
	,"AssessmentTarget"."TargetTemplateID" "TargetTemplateID"
	,"TargetTemplate"."TargetName" "TargetTemplateTargetName"
	,"TargetTemplateMeasurementType"."MeasurementType" "TargetTemplateMeasurementType"
	,"TargetTemplateTargetLevel"."TargetLevel" "TargetTemplateTargetLevel"
	,"AssessmentTarget"."TargetLevelID" "TargetLevelID"
	,"TargetLevel"."TargetLevel" "TargetLevel"
	,"AssessmentTarget"."TargetName" "TargetName"
	,"AssessmentTarget"."Description" "Description"
	,"AssessmentTarget"."TargetValue" "TargetValue"
	,"AssessmentTarget"."MeasurementTypeID" "MeasurementTypeID"
	,"MeasurementType"."MeasurementType" "MeasurementType"
	,"AssessmentTarget"."CourseID" "CourseID"
	,"Course"."CourseTitle" "CourseTitle"
	,"AssessmentTarget"."CalendarSessionID" "CalendarSessionID"
	,"Session"."SessionCode" "SessionCode"
	,"Session"."SessionName" "SessionName"
	,"AssessmentTarget"."EffectiveDate" "EffectiveDate"
	,"AssessmentTarget"."EffectiveEndDate" "EffectiveEndDate"
	,"AssessmentTarget"."IsActive" "IsActive"
	,"AssessmentTarget"."CreatedBy" "CreatedBy"
	,"AssessmentTarget"."CreatedDate" "CreatedDate"
	,"AssessmentTarget"."ModifiedDate" "ModifiedDate"
	,"AssessmentTarget"."TargetType" "TargetType"
	,"AssessmentTarget"."Weighting" "Weighting"
	,"AssessmentTarget"."TargetLevel" "extTargetLevel"
from "dbo"."AssessmentTarget" "AssessmentTarget"
	left join "dbo"."zlkpTargetTemplate" "TargetTemplate" on "AssessmentTarget"."TargetTemplateID" = "TargetTemplate"."TargetTemplateID"
	left join "dbo"."zlkpMeasurementType" "TargetTemplateMeasurementType" on "TargetTemplate"."MeasurementTypeID" = "TargetTemplateMeasurementType"."MeasurementTypeID"
	left join "dbo"."zlkpTargetLevel" "TargetTemplateTargetLevel" on "TargetTemplate"."TargetLevelID" = "TargetTemplateTargetLevel"."TargetLevelID"
	left join "dbo"."zlkpTargetLevel" "TargetLevel" on "AssessmentTarget"."TargetLevelID" = "TargetLevel"."TargetLevelID"
	left join "dbo"."zlkpMeasurementType" "MeasurementType" on "AssessmentTarget"."MeasurementTypeID" = "MeasurementType"."MeasurementTypeID"
	left join "dbo"."Session" "Session" on "AssessmentTarget"."CalendarSessionID" = "Session"."CalendarSessionID"
	left join "dbo"."Course" "Course" on "AssessmentTarget"."CourseID" = "Course"."CourseID"
]]></text>
    </command>
    <command id="AssessmentTargetIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AssessmentTargetID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="AssessmentTargetID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Assessment Target ID" readOnly="true" />
    <field name="TargetTemplateID" type="Int32" label="Target Template ID" showInSummary="true">
      <items style="Lookup" dataController="TargetTemplate" newDataView="createForm1" dataValueField="TargetTemplateID" dataTextField="TargetName" copy="TargetTemplateMeasurementType=MeasurementType, TargetTemplateTargetLevel=TargetLevel" />
    </field>
    <field name="TargetTemplateTargetName" type="String" readOnly="true" label="Target Name" length="35" />
    <field name="TargetTemplateMeasurementType" type="String" readOnly="true" label="Measurement Type" length="50" />
    <field name="TargetTemplateTargetLevel" type="String" readOnly="true" label="Target Level" length="20" />
    <field name="TargetLevelID" type="Int32" label="Target Level ID" showInSummary="true">
      <items style="Lookup" dataController="TargetLevel" newDataView="createForm1" dataValueField="TargetLevelID" dataTextField="TargetLevel" />
    </field>
    <field name="TargetLevel" type="String" readOnly="true" label="Target Level" length="20" />
    <field name="TargetName" type="String" label="Target Name" length="50" showInSummary="true" />
    <field name="Description" type="String" label="Description" length="255" showInSummary="true" />
    <field name="TargetValue" type="Decimal" label="Target Value" showInSummary="true" />
    <field name="MeasurementTypeID" type="Int32" label="Measurement Type ID">
      <items style="Lookup" dataController="MeasurementType" newDataView="createForm1" dataValueField="MeasurementTypeID" dataTextField="MeasurementType" />
    </field>
    <field name="MeasurementType" type="String" readOnly="true" label="Measurement Type" length="20" />
    <field name="CourseID" type="Int32" label="Course ID">
      <items style="Lookup" dataController="Course" newDataView="createForm1" dataValueField="CourseID" dataTextField="Course" />
    </field>
    <field name="CourseTitle" type="String" readOnly="true" label="Course" length="20" />
    <field name="CalendarSessionID" type="Int32" label="Calendar Session ID">
      <items style="Lookup" dataController="Session" newDataView="createForm1" dataValueField="CalendarSessionID" dataTextField="SessionCode" copy="SessionName=SessionName" />
    </field>
    <field name="SessionCode" type="String" readOnly="true" label="Session Code" length="20" />
    <field name="SessionName" type="String" readOnly="true" label="Session Name" length="50" />
    <field name="EffectiveDate" type="DateTime" default="(getdate())" label="Effective Date" />
    <field name="EffectiveEndDate" type="DateTime" label="Effective End Date" />
    <field name="IsActive" type="Boolean" default="((1))" label="Is Active" />
    <field name="CreatedBy" type="String" label="Created By" length="100" />
    <field name="CreatedDate" type="DateTime" dataFormatString="g" default="(getdate())" label="Created Date" />
    <field name="ModifiedDate" type="DateTime" dataFormatString="g" default="(getdate())" label="Modified Date" />
    <field name="TargetType" type="String" label="Target Type" length="50" />
    <field name="Weighting" type="Decimal" label="Weighting" />
    <field name="extTargetLevel" type="String" label="extTarget Level" length="30" />
  </fields>
  <views>
