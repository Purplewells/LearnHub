    <field name="AccountType" type="String" readOnly="true" label="Account Type" length="20" />
    <field name="DebitEffect" type="Int32" allowNulls="false" label="Debit Effect" showInSummary="true" />
    <field name="CreditEffect" type="Int32" allowNulls="false" label="Credit Effect" showInSummary="true" />
    <field name="IsNetIncome" type="Boolean" allowNulls="false" default="((0))" label="Is Net Income" showInSummary="true" />
    <field name="Description" type="String" label="Description" length="255" showInSummary="true" />
    <field name="IsActive" type="Boolean" default="((1))" label="Is Active" />
  </fields>
