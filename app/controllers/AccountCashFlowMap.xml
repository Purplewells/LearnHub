<?xml version="1.0" encoding="utf-8"?>
<dataController name="AccountCashFlowMap" conflictDetection="overwriteChanges" label="Account Cash Flow Map" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"zlkpAccountCashFlowMap"."AccountCashFlowMapID" "AccountCashFlowMapID"
	,"zlkpAccountCashFlowMap"."AccountTypeID" "AccountTypeID"
	,"AccountType"."AccountType" "AccountType"
	,"zlkpAccountCashFlowMap"."DebitEffect" "DebitEffect"
	,"zlkpAccountCashFlowMap"."CreditEffect" "CreditEffect"
	,"zlkpAccountCashFlowMap"."IsNetIncome" "IsNetIncome"
	,"zlkpAccountCashFlowMap"."Description" "Description"
	,"zlkpAccountCashFlowMap"."IsActive" "IsActive"
from "dbo"."zlkpAccountCashFlowMap" "zlkpAccountCashFlowMap"
	left join "dbo"."account_type" "AccountType" on "zlkpAccountCashFlowMap"."AccountTypeID" = "AccountType"."AccountTypeID"
]]></text>
    </command>
    <command id="AccountCashFlowMapIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AccountCashFlowMapID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="AccountCashFlowMapID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Account Cash Flow Map ID" readOnly="true" />
    <field name="AccountTypeID" type="Int32" allowNulls="false" label="Account Type ID" showInSummary="true">
      <items style="Lookup" dataController="AccountType" newDataView="createForm1" dataValueField="AccountTypeID" dataTextField="AccountType" />
