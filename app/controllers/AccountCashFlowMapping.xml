<?xml version="1.0" encoding="utf-8"?>
<dataController name="AccountCashFlowMapping" conflictDetection="overwriteChanges" label="Account Cash Flow Mapping" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"zmap_AccountCashFlow"."AccountID" "AccountID"
	,"Account"."AccountName" "AccountName"
	,"AccountType"."AccountType" "AccountType"
	,"AccountSubCategory"."AccountSubCategory" "AccountSubCategory"
	,"AccountCategory"."AccountCategory" "AccountCategory"
	,"AccountUsage"."AccountUsage" "AccountUsage"
	,"zmap_AccountCashFlow"."CashFlowCategoryID" "CashFlowCategoryID"
	,"CashFlowCategory"."CashFlowCategory" "CashFlowCategory"
	,"zmap_AccountCashFlow"."IsActive" "IsActive"
	,"zmap_AccountCashFlow"."CreatedDate" "CreatedDate"
	,"zmap_AccountCashFlow"."ModifiedDate" "ModifiedDate"
from "dbo"."zmap_AccountCashFlow" "zmap_AccountCashFlow"
	left join "dbo"."chart_of_accounts" "Account" on "zmap_AccountCashFlow"."AccountID" = "Account"."AccountID"
	left join "dbo"."account_type" "AccountType" on "Account"."AccountTypeID" = "AccountType"."AccountTypeID"
	left join "dbo"."zlkpAccountSubCategory" "AccountSubCategory" on "Account"."AccountSubCategoryID" = "AccountSubCategory"."AccountSubCategoryID"
	left join "dbo"."zlkpChartAccountCategory" "AccountCategory" on "Account"."AccountCategoryID" = "AccountCategory"."AccountCategoryID"
	left join "dbo"."zlkpChartAccountUsage" "AccountUsage" on "Account"."AccountUsageID" = "AccountUsage"."AccountUsageID"
	left join "dbo"."zlkpCashFlowCategory" "CashFlowCategory" on "zmap_AccountCashFlow"."CashFlowCategoryID" = "CashFlowCategory"."CashFlowCategoryID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="AccountID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Account ID" showInSummary="true">
      <items style="Lookup" dataController="ChartOfAccounts" newDataView="createForm1" dataValueField="AccountID" dataTextField="AccountName" copy="AccountType=AccountType, AccountSubCategory=AccountSubCategory, AccountCategory=AccountCategory, AccountUsage=AccountUsage" />
    </field>
    <field name="AccountName" type="String" readOnly="true" label="Account Name" length="100" />
    <field name="AccountType" type="String" readOnly="true" label="Account Type" length="20" />
    <field name="AccountSubCategory" type="String" readOnly="true" label="Account Sub Category" length="40" />
    <field name="AccountCategory" type="String" readOnly="true" label="Account Category" length="50" />
    <field name="AccountUsage" type="String" readOnly="true" label="Account Usage" length="25" />
    <field name="CashFlowCategoryID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Cash Flow Category ID" showInSummary="true">
      <items style="Lookup" dataController="CashFlowCategory" newDataView="createForm1" dataValueField="CashFlowCategoryID" dataTextField="CashFlowCategory" />
    </field>
    <field name="CashFlowCategory" type="String" readOnly="true" label="Cash Flow Category" length="50" />
    <field name="IsActive" type="Boolean" allowNulls="false" default="((1))" label="Is Active" showInSummary="true" />
    <field name="CreatedDate" type="DateTime" allowNulls="false" dataFormatString="g" default="(getutcdate())" label="Created Date" showInSummary="true" />
    <field name="ModifiedDate" type="DateTime" allowNulls="false" dataFormatString="g" default="(getutcdate())" label="Modified Date" showInSummary="true" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Account Cash Flow Mapping">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="AccountID" aliasFieldName="AccountName" />
        <dataField fieldName="AccountType" columns="20" />
        <dataField fieldName="AccountSubCategory" columns="40" />
        <dataField fieldName="AccountCategory" columns="50" />
        <dataField fieldName="AccountUsage" columns="25" />
        <dataField fieldName="CashFlowCategoryID" aliasFieldName="CashFlowCategory" />
        <dataField fieldName="IsActive" />
        <dataField fieldName="CreatedDate" columns="20" />
        <dataField fieldName="ModifiedDate" columns="20" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Account Cash Flow Mapping">
