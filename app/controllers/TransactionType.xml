<?xml version="1.0" encoding="utf-8"?>
<dataController name="TransactionType" conflictDetection="overwriteChanges" label="Transaction Type" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"zlkpTransactionType"."TransactionTypeID" "TransactionTypeID"
	,"zlkpTransactionType"."TransactionType" "TransactionType"
	,"zlkpTransactionType"."TransactionTypeCode" "TransactionTypeCode"
	,"zlkpTransactionType"."IsActive" "IsActive"
	,"zlkpTransactionType"."SortOrder" "SortOrder"
	,"zlkpTransactionType"."CreatedDate" "CreatedDate"
	,"zlkpTransactionType"."ModifiedDate" "ModifiedDate"
	,"zlkpTransactionType"."CreatedBy" "CreatedBy"
	,"zlkpTransactionType"."ModifiedBy" "ModifiedBy"
from "dbo"."zlkpTransactionType" "zlkpTransactionType"
]]></text>
    </command>
    <command id="TransactionTypeIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="TransactionTypeID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="TransactionTypeID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Transaction Type ID" readOnly="true" />
    <field name="TransactionType" type="String" label="Transaction Type" length="20" showInSummary="true" />
    <field name="TransactionTypeCode" type="String" label="Code" length="20" showInSummary="true" />
    <field name="IsActive" type="Boolean" default="((1))" label="Active" showInSummary="true" />
    <field name="SortOrder" type="Int32" label="Sort Order" showInSummary="true" />
    <field name="CreatedDate" type="DateTime" dataFormatString="g" label="Created Date" showInSummary="true" />
    <field name="ModifiedDate" type="DateTime" dataFormatString="g" label="Modified Date" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Active Transaction Types" filter="IsActive = 1">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="TransactionType" columns="20" />
        <dataField fieldName="TransactionTypeCode" columns="20">
          <headerText>Code</headerText>
        </dataField>
        <dataField fieldName="IsActive" />
        <dataField fieldName="SortOrder" columns="15" />
        <dataField fieldName="CreatedDate" columns="20" />
        <dataField fieldName="ModifiedDate" columns="20" />
        <dataField fieldName="CreatedBy" columns="50" hidden="true" />
        <dataField fieldName="ModifiedBy" columns="50" hidden="true" />
      </dataFields>
    </view>
    <view id="all_transaction_types" type="Grid" commandId="command1" label="All Transaction Types">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="TransactionType" columns="20" />
        <dataField fieldName="TransactionTypeCode" columns="20" />
        <dataField fieldName="IsActive" />
        <dataField fieldName="SortOrder" columns="15" />
        <dataField fieldName="CreatedDate" columns="20" hidden="true" />
        <dataField fieldName="ModifiedDate" columns="20" hidden="true" />
        <dataField fieldName="CreatedBy" columns="50" hidden="true" />
        <dataField fieldName="ModifiedBy" columns="50" hidden="true" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Transaction Type">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Transaction Type" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="TransactionType" columns="20" />
            <dataField fieldName="TransactionTypeCode" columns="20" />
            <dataField fieldName="IsActive" />
            <dataField fieldName="SortOrder" columns="15" />
            <dataField fieldName="CreatedDate" columns="20" />
            <dataField fieldName="ModifiedDate" columns="20" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="ModifiedBy" columns="50" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Transaction Type">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="TransactionType" columns="20" />
            <dataField fieldName="TransactionTypeCode" columns="20" />
            <dataField fieldName="IsActive" />
            <dataField fieldName="SortOrder" columns="15" />
            <dataField fieldName="CreatedDate" columns="20" />
            <dataField fieldName="ModifiedDate" columns="20" />
            <dataField fieldName="CreatedBy" columns="50" />
            <dataField fieldName="ModifiedBy" columns="50" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
