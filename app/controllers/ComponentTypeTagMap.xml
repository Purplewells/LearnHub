<?xml version="1.0" encoding="utf-8"?>
<dataController name="ComponentTypeTagMap" conflictDetection="overwriteChanges" label="Component Type Tag Map" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"zrefComponentTypeTagMap"."TagID" "TagID"
	,"Tag"."TagName" "TagName"
	,"Tag"."TagCode" "TagCode"
	,"zrefComponentTypeTagMap"."ElementID" "ElementID"
	,"Element"."ElementName" "ElementName"
	,"zrefComponentTypeTagMap"."IsActive" "IsActive"
	,"zrefComponentTypeTagMap"."EffectiveStartDate" "EffectiveStartDate"
	,"zrefComponentTypeTagMap"."EffectiveEndDate" "EffectiveEndDate"
	,"zrefComponentTypeTagMap"."CreatedDate" "CreatedDate"
	,"zrefComponentTypeTagMap"."CreatedBy" "CreatedBy"
	,"PayrollCategory"."TagCategoryName" "TagCategoryName"
	,"PayrollCategory"."TagCategoryCode" "CategoryCode"
from "dbo"."zrefComponentTypeTagMap" "zrefComponentTypeTagMap"
	left join "dbo"."zlkpPayrollTags" "Tag" on "zrefComponentTypeTagMap"."TagID" = "Tag"."TagID"
	left join "dbo"."zlkpPayrollTagCategory" "PayrollCategory" on "Tag"."PayrollCategoryID" = "PayrollCategory"."PayrollCategoryID"
	left join "dbo"."zlkpSalaryComponents" "Element" on "zrefComponentTypeTagMap"."ElementID" = "Element"."ElementID"
]]></text>
    </command>
  </commands>
  <fields>
    <field name="TagID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Tag ID" showInSummary="true">
      <items style="Lookup" dataController="PayrollTags" newDataView="createForm1" dataValueField="TagID" dataTextField="TagName" copy="TagCode=TagCode, TagCategoryName=CategoryName, CategoryCode=CategoryCode" />
    </field>
    <field name="TagName" type="String" readOnly="true" label="Tag Name" length="30" />
    <field name="TagCode" type="String" readOnly="true" label="Tag Code" length="30" />
    <field name="ElementID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Element ID" showInSummary="true">
      <items style="Lookup" dataController="SalaryComponent" newDataView="createForm1" dataValueField="ElementID" dataTextField="ElementName" />
    </field>
    <field name="ElementName" type="String" readOnly="true" label="Element Name" length="40" />
    <field name="IsActive" type="Boolean" default="((1))" label="Active" showInSummary="true" />
    <field name="EffectiveStartDate" type="DateTime" dataFormatString="d" label="Effective Start Date" showInSummary="true" />
    <field name="EffectiveEndDate" type="DateTime" dataFormatString="d" label="Effective End Date" showInSummary="true" />
    <field name="CreatedDate" type="DateTime" dataFormatString="g" label="Created Date" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="TagCategoryName" type="String" readOnly="true" label="Tag Category Name" length="100" />
    <field name="CategoryCode" type="String" readOnly="true" label="Category Code" length="50" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Component and Tag Mapping">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="ElementID" aliasFieldName="ElementName" />
        <dataField fieldName="TagID" aliasFieldName="TagName" />
        <dataField fieldName="TagCode" columns="30" />
        <dataField fieldName="CategoryCode" columns="30" />
        <dataField fieldName="TagCategoryName" columns="30">
          <headerText>Tag Category</headerText>
        </dataField>
        <dataField fieldName="IsActive" />
        <dataField fieldName="EffectiveStartDate" columns="20" />
        <dataField fieldName="EffectiveEndDate" columns="20" />
        <dataField fieldName="CreatedDate" columns="20" hidden="true" />
        <dataField fieldName="CreatedBy" columns="50" hidden="true" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Enter and Maintain Component &amp; Tag Mapping">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Component Type Tag Map" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="ElementID" aliasFieldName="ElementName" />
            <dataField fieldName="TagID" aliasFieldName="TagName" />
            <dataField fieldName="TagCode" columns="30" />
            <dataField fieldName="IsActive" />
            <dataField fieldName="TagCategoryName" />
            <dataField fieldName="CategoryCode" columns="30" />
            <dataField fieldName="EffectiveStartDate" columns="10" />
            <dataField fieldName="EffectiveEndDate" columns="10" />
            <dataField fieldName="CreatedDate" columns="20" />
            <dataField fieldName="CreatedBy" columns="50" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Component Tag Mapping">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="ElementID" aliasFieldName="ElementName" />
            <dataField fieldName="TagID" aliasFieldName="TagName" />
            <dataField fieldName="TagCategoryName" />
            <dataField fieldName="TagCode" columns="30" />
            <dataField fieldName="IsActive" />
            <dataField fieldName="CategoryCode" columns="50" />
            <dataField fieldName="EffectiveStartDate" columns="10" />
            <dataField fieldName="EffectiveEndDate" columns="10" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
  <actions>
    <actionGroup id="ag1" scope="Grid">
      <action id="a1" commandName="Select" commandArgument="editForm1" />
      <action id="a2" commandName="Edit" />
      <action id="a3" commandName="Delete" />
      <action id="a6" />
      <action id="a7" commandName="New" commandArgument="grid1" />
      <action id="a8" commandName="Duplicate" commandArgument="createForm1" />
      <action id="a9" />
      <action id="a10" commandName="BatchEdit" commandArgument="editForm1" />
    </actionGroup>
    <actionGroup id="ag2" scope="Form">
      <action id="a1" commandName="Edit" />
      <action id="a2" commandName="Delete" />
      <action id="a3" commandName="Cancel" />
      <action id="a4" whenLastCommandName="Edit" commandName="Update" commandArgument="Save" />
      <action id="a5" whenLastCommandName="Edit" commandName="Cancel" />
      <action id="a6" whenLastCommandName="New" commandName="Insert" commandArgument="Save" />
      <action id="a7" whenLastCommandName="New" commandName="Insert" commandArgument="SaveAndNew" />
      <action id="a8" whenLastCommandName="New" commandName="Cancel" />
      <action id="a9" whenLastCommandName="Duplicate" commandName="Insert" />
      <action id="a10" whenLastCommandName="Duplicate" commandName="Cancel" />
      <action id="a13" whenLastCommandName="Insert" whenLastCommandArgument="Save" whenView="createForm1" commandName="Select" commandArgument="editForm1" whenClientScript="this.hasDetails()" />
      <action id="a14" whenLastCommandName="Insert" whenLastCommandArgument="SaveAndNew" commandName="New" commandArgument="createForm1" />
    </actionGroup>
    <actionGroup id="ag3" scope="ActionBar" headerText="New" flat="true">
      <action id="a1" commandName="New" commandArgument="createForm1" cssClass="NewIcon" />
    </actionGroup>
    <actionGroup id="ag4" scope="ActionBar" headerText="Edit/Delete" flat="true">
      <action id="a1" whenKeySelected="true" commandName="Edit" commandArgument="editForm1" cssClass="EditIcon" whenView="grid1" />
      <action id="a2" whenKeySelected="true" commandName="Delete" cssClass="DeleteIcon" whenView="grid1" />
    </actionGroup>
    <actionGroup id="ag5" scope="ActionBar" headerText="Actions">
      <action id="a1" commandName="ExportCsv" />
      <action id="a2" />
      <action id="a3" commandName="ExportRowset" />
      <action id="a4" commandName="ExportRss" />
