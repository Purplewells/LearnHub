    <view id="createForm1" type="Form" commandId="command1" label="New Target Adjustment">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="AssignmentID" aliasFieldName="AssignmentEntityType" />
            <dataField fieldName="AssignmentTargetLevel" columns="30" />
