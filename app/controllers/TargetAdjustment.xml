<?xml version="1.0" encoding="utf-8"?>
<dataController name="TargetAdjustment" conflictDetection="overwriteChanges" label="Target Adjustment" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"TargetAdjustment"."AdjustmentID" "AdjustmentID"
	,"TargetAdjustment"."AssignmentID" "AssignmentID"
	,"Assignment"."EntityType" "AssignmentEntityType"
	,"AssignmentTarget"."TargetLevel" "AssignmentTargetLevel"
	,"TargetAdjustment"."AdjustedTargetValue" "AdjustedTargetValue"
	,"TargetAdjustment"."AdjustedWeighting" "AdjustedWeighting"
	,"TargetAdjustment"."AdjustmentReason" "AdjustmentReason"
	,"TargetAdjustment"."AdjustmentReasonID" "AdjustmentReasonID"
	,"TargetAdjustment"."ApprovedBy" "ApprovedBy"
	,"TargetAdjustment"."ApprovedDate" "ApprovedDate"
	,"TargetAdjustment"."IsActive" "IsActive"
	,"TargetAdjustment"."CreatedDate" "CreatedDate"
	,"TargetAdjustment"."ModifiedDate" "ModifiedDate"
	,"TargetAdjustment"."CreatedBy" "CreatedBy"
	,"TargetAdjustment"."ModifiedBy" "ModifiedBy"
from "dbo"."TargetAdjustment" "TargetAdjustment"
	left join "dbo"."TargetAssignment" "Assignment" on "TargetAdjustment"."AssignmentID" = "Assignment"."AssignmentID"
	left join "dbo"."AssessmentTarget" "AssignmentTarget" on "Assignment"."TargetID" = "AssignmentTarget"."AssessmentTargetID"
]]></text>
    </command>
    <command id="AdjustmentIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="AdjustmentID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="AdjustmentID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Adjustment ID" readOnly="true" />
    <field name="AssignmentID" type="Int32" allowNulls="false" label="Assignment ID" showInSummary="true">
      <items style="Lookup" dataController="TargetAssignment" newDataView="createForm1" dataValueField="AssignmentID" dataTextField="EntityType" copy="AssignmentTargetLevel=TargetLevel" />
    </field>
    <field name="AssignmentEntityType" type="String" readOnly="true" label="Assignment Entity Type" length="50" />
    <field name="AssignmentTargetLevel" type="String" readOnly="true" label="Assignment Target Level" length="30" />
    <field name="AdjustedTargetValue" type="Decimal" label="Adjusted Target Value" showInSummary="true" />
    <field name="AdjustedWeighting" type="Decimal" label="Adjusted Weighting" showInSummary="true" />
    <field name="AdjustmentReason" type="String" label="Adjustment Reason" length="100" showInSummary="true" />
    <field name="AdjustmentReasonID" type="Int32" label="Adjustment Reason ID" showInSummary="true" />
    <field name="ApprovedBy" type="String" label="Approved By" length="50" />
    <field name="ApprovedDate" type="DateTime" dataFormatString="g" label="Approved Date" />
    <field name="IsActive" type="Boolean" default="((1))" label="Is Active" />
    <field name="CreatedDate" type="DateTime" dataFormatString="g" default="(getdate())" label="Created Date" />
    <field name="ModifiedDate" type="DateTime" dataFormatString="g" default="(getdate())" label="Modified Date" />
    <field name="CreatedBy" type="String" label="Created By" length="50" />
    <field name="ModifiedBy" type="String" label="Modified By" length="50" />
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Target Adjustment">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="AssignmentID" aliasFieldName="AssignmentEntityType" />
        <dataField fieldName="AssignmentTargetLevel" columns="30" />
        <dataField fieldName="AdjustedTargetValue" columns="15" />
        <dataField fieldName="AdjustedWeighting" columns="15" />
        <dataField fieldName="AdjustmentReason" />
        <dataField fieldName="AdjustmentReasonID" columns="15" />
        <dataField fieldName="ApprovedBy" columns="50" />
        <dataField fieldName="ApprovedDate" columns="20" />
        <dataField fieldName="IsActive" />
        <dataField fieldName="CreatedDate" columns="20" />
        <dataField fieldName="ModifiedDate" columns="20" />
        <dataField fieldName="CreatedBy" columns="50" />
        <dataField fieldName="ModifiedBy" columns="50" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Target Adjustment">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
