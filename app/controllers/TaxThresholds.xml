      <text><![CDATA[
select
	"TaxThresholds"."TaxThresholdID" "TaxThresholdID"
	,"TaxThresholds"."TaxType" "TaxType"
	,"TaxThresholds"."TaxTypeID" "TaxTypeID"
	,"TaxThresholds"."MinAmount" "MinAmount"
