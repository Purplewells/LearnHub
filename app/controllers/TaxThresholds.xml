	,"TaxThresholds"."EffectiveToDate" "EffectiveToDate"
	,"TaxThresholds"."CreatedDate" "CreatedDate"
	,"TaxThresholds"."ModifiedDate" "ModifiedDate"
	,"TaxThresholds"."CreatedBy" "CreatedBy"
	,"TaxThresholds"."ModifiedBy" "ModifiedBy"
from "dbo"."TaxThresholds" "TaxThresholds"
]]></text>
    </command>
    <command id="TaxThresholdIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="TaxThresholdID" />
      </output>
    </command>
  </commands>
