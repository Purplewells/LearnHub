    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Inventory Transactions">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Inventory Transactions" flow="NewColumn">
