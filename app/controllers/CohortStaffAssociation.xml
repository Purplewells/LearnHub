<?xml version="1.0" encoding="utf-8"?>
<dataController name="CohortStaffAssociation" conflictDetection="overwriteChanges" label="Cohort Staff Association" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"CohortStaffAssociation"."CohortID" "CohortID"
	,"Cohort"."CohortDescription" "CohortDescription"
	,"CohortStaffAssociation"."StartDate" "StartDate"
	,"CohortStaffAssociation"."EndDate" "EndDate"
	,"CohortStaffAssociation"."StaffAssignmentID" "StaffAssignmentID"
	,"StaffAssignment"."StaffClassificationID" "StaffAssignmentStaffClassificationID"
	,"Person"."FirstName" "FirstName"
	,"Person"."LastName" "LastName"
from "dbo"."CohortStaffAssociation" "CohortStaffAssociation"
	left join "dbo"."Cohort" "Cohort" on "CohortStaffAssociation"."CohortID" = "Cohort"."CohortID"
	left join "dbo"."K12StaffAssignment" "StaffAssignment" on "CohortStaffAssociation"."StaffAssignmentID" = "StaffAssignment"."StaffAssignmentID"
	left join "dbo"."OrganizationPersonRole" "OrganizationPersonRole" on "StaffAssignment"."OrganizationPersonRoleID" = "OrganizationPersonRole"."OrganizationPersonRoleID"
	left join "dbo"."Person" "Person" on "OrganizationPersonRole"."PersonID" = "Person"."PersonID"
]]></text>
    </command>
    <command id="StaffCohortAssociationIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="CohortID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Cohort ID" showInSummary="true">
      <items style="Lookup" dataController="Cohort" newDataView="createForm1" dataValueField="CohortID" dataTextField="CohortDescription" />
    </field>
    <field name="CohortDescription" type="String" readOnly="true" label="Cohort Description" length="50" />
    <field name="StartDate" type="DateTime" label="Start Date" showInSummary="true" />
    <field name="EndDate" type="DateTime" label="End Date" showInSummary="true" />
    <field name="StaffAssignmentID" type="Int32" allowNulls="false" label="Staff Assignment ID" showInSummary="true">
      <items style="Lookup" dataController="zlkpStaffList" dataValueField="PersonID" dataTextField="StaffNameTitle" copy="FirstName=FirstName, LastName=LastName" dataView="grid1" />
    </field>
    <field name="StaffAssignmentStaffClassificationID" type="Int32" readOnly="true" label="StaffAssignmentStaffClassificationID" />
    <field name="FirstName" type="String" readOnly="true" label="Firstname" length="35" />
    <field name="LastName" type="String" readOnly="true" label="Lastname" length="35" />
    <field name="StaffName" type="String" readOnly="true" label="Staff Name" computed="true" showInSummary="true">
      <formula>Person.FirstName + ' ' +Person.LastName</formula>
    </field>
  </fields>
  <views>
    <view id="grid1" type="Grid" commandId="command1" label="Cohort Staff Association">
      <headerText>$DefaultGridViewDescription</headerText>
      <dataFields>
        <dataField fieldName="CohortID" aliasFieldName="CohortDescription" />
        <dataField fieldName="StartDate" columns="10" />
        <dataField fieldName="EndDate" columns="10" />
        <dataField fieldName="StaffAssignmentID" aliasFieldName="StaffAssignmentStaffClassificationID" />
        <dataField fieldName="FirstName" columns="35" />
        <dataField fieldName="LastName" columns="35" />
        <dataField fieldName="StaffName" />
      </dataFields>
    </view>
    <view id="editForm1" type="Form" commandId="command1" label="Review Intervention Staff">
      <headerText>$DefaultEditViewDescription</headerText>
      <categories>
        <category id="c1" headerText="Cohort Staff Association" flow="NewColumn">
          <description><![CDATA[$DefaultEditDescription]]></description>
          <dataFields>
            <dataField fieldName="CohortID" aliasFieldName="CohortDescription" />
            <dataField fieldName="StartDate" columns="10" />
            <dataField fieldName="EndDate" columns="10" />
            <dataField fieldName="StaffAssignmentID" aliasFieldName="StaffName" />
            <dataField fieldName="StaffName" />
          </dataFields>
        </category>
      </categories>
    </view>
    <view id="createForm1" type="Form" commandId="command1" label="New Intervention Staff">
      <headerText>$DefaultCreateViewDescription</headerText>
      <categories>
        <category id="c1" flow="NewColumn">
          <description><![CDATA[$DefaultNewDescription]]></description>
          <dataFields>
            <dataField fieldName="CohortID" aliasFieldName="CohortDescription" />
            <dataField fieldName="StartDate" columns="10" />
            <dataField fieldName="EndDate" columns="10" />
            <dataField fieldName="StaffAssignmentID" aliasFieldName="StaffName" />
            <dataField fieldName="StaffName" />
          </dataFields>
        </category>
      </categories>
    </view>
  </views>
