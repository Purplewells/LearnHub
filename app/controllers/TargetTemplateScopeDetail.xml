<?xml version="1.0" encoding="utf-8"?>
<dataController name="TargetTemplateScopeDetail" conflictDetection="overwriteChanges" label="Target Template Scope Detail" plugIn="zLearnHub.Data.AnnotationPlugIn" xmlns="urn:schemas-codeontime-com:data-aquarium">
  <commands>
    <command id="command1" type="Text">
      <text><![CDATA[
select
	"zlkpTargetTemplateScope"."TargetTemplateScopeID" "TargetTemplateScopeID"
	,"zlkpTargetTemplateScope"."TargetTemplateID" "TargetTemplateID"
	,"TargetTemplate"."TargetName" "TargetTemplateTargetName"
	,"TargetTemplateMeasurementType"."MeasurementType" "TargetTemplateMeasurementType"
	,"TargetTemplateMeasurementUnit"."MeasurementUnitName" "TargetTemplateMeasurementUnitName"
	,"TargetTemplateTargetLevel"."TargetLevel" "TargetTemplateTargetLevel"
	,"zlkpTargetTemplateScope"."EntityType" "EntityType"
	,"zlkpTargetTemplateScope"."TargetEntityID" "TargetEntityID"
	,"TargetEntity"."EntityCode" "TargetEntityCode"
	,"zlkpTargetTemplateScope"."IsGlobalScope" "IsGlobalScope"
	,"zlkpTargetTemplateScope"."IsActive" "IsActive"
	,"zlkpTargetTemplateScope"."EffectiveStartDate" "EffectiveStartDate"
	,"zlkpTargetTemplateScope"."EffectiveEndDate" "EffectiveEndDate"
	,"zlkpTargetTemplateScope"."CreatedBy" "CreatedBy"
	,"zlkpTargetTemplateScope"."CreatedDate" "CreatedDate"
	,"zlkpTargetTemplateScope"."ModifiedBy" "ModifiedBy"
	,"zlkpTargetTemplateScope"."ModifiedDate" "ModifiedDate"
from "dbo"."zlkpTargetTemplateScope" "zlkpTargetTemplateScope"
	left join "dbo"."zlkpTargetTemplate" "TargetTemplate" on "zlkpTargetTemplateScope"."TargetTemplateID" = "TargetTemplate"."TargetTemplateID"
	left join "dbo"."zlkpMeasurementType" "TargetTemplateMeasurementType" on "TargetTemplate"."MeasurementTypeID" = "TargetTemplateMeasurementType"."MeasurementTypeID"
	left join "dbo"."zlkpMeasurementUnit" "TargetTemplateMeasurementUnit" on "TargetTemplate"."MeasurementUnitID" = "TargetTemplateMeasurementUnit"."MeasurementUnitID"
	left join "dbo"."zlkpTargetLevel" "TargetTemplateTargetLevel" on "TargetTemplate"."TargetLevelID" = "TargetTemplateTargetLevel"."TargetLevelID"
	left join "dbo"."zlkpTargetEntity" "TargetEntity" on "zlkpTargetTemplateScope"."TargetEntityID" = "TargetEntity"."TargetEntityID"
]]></text>
    </command>
    <command id="TargetTemplateScopeIDIdentityCommand" type="Text" event="Inserted">
      <text><![CDATA[select @@identity]]></text>
      <output>
        <fieldOutput fieldName="TargetTemplateScopeID" />
      </output>
    </command>
  </commands>
  <fields>
    <field name="TargetTemplateScopeID" type="Int32" allowNulls="false" isPrimaryKey="true" label="Target Template Scope ID" readOnly="true" />
    <field name="TargetTemplateID" type="Int32" allowNulls="false" label="Target Template ID" showInSummary="true">
      <items style="Lookup" dataController="TargetTemplate" newDataView="createForm1" dataValueField="TargetTemplateID" dataTextField="TargetName" copy="TargetTemplateMeasurementType=MeasurementType, TargetTemplateMeasurementUnitName=MeasurementUnitName, TargetTemplateTargetLevel=TargetLevel" />
    </field>
    <field name="TargetTemplateTargetName" type="String" readOnly="true" label="Target Name" length="200" />
    <field name="TargetTemplateMeasurementType" type="String" readOnly="true" label="Measurement Type" length="50" />
    <field name="TargetTemplateMeasurementUnitName" type="String" readOnly="true" label="Measurement Unit" length="100" />
    <field name="TargetTemplateTargetLevel" type="String" readOnly="true" label="Target Level" length="20" />
    <field name="EntityType" type="String" label="Entity Type" length="50" showInSummary="true" />
    <field name="TargetEntityID" type="Int32" label="Target Entity ID" showInSummary="true">
      <items style="Lookup" dataController="TargetEntity" newDataView="createForm1" dataValueField="TargetEntityID" dataTextField="EntityCode" />
    </field>
    <field name="TargetEntityCode" type="String" readOnly="true" label="Entity Code" length="50" />
    <field name="IsGlobalScope" type="Boolean" default="((0))" label="Is GlobalScope" showInSummary="true" />
    <field name="IsActive" type="Boolean" default="((1))" label="Is Active" showInSummary="true" />
    <field name="EffectiveStartDate" type="DateTime" dataFormatString="d" default="(getdate())" label="Effective Start Date" />
    <field name="EffectiveEndDate" type="DateTime" dataFormatString="d" default="(dateadd(year,(99),getdate()))" label="Effective End Date" />
    <field name="CreatedBy" type="String" default="(suser_sname())" label="Created By" length="100" />
    <field name="CreatedDate" type="DateTime" dataFormatString="g" default="(getdate())" label="Created Date" />
    <field name="ModifiedBy" type="String" label="Modified By" length="100" />
    <field name="ModifiedDate" type="DateTime" dataFormatString="g" label="Modified Date" />
  </fields>
